    function restaurar() {
        var w = screen.width;
        var h = screen.height;
        window.statusbar.visible = false;
        window.scrollbars.visible = false;
        window.menubar.visible = false;
        window.toolbar.visible = false;
        window.locationbar.visible = false;
        window.personalbar.visible = false;
        //window.directories.visible = false;
        window.screenX = w + 300;
        window.screenY = 0;
        window.innerWidth = w;
        window.innerHeight = h;
        //alwaysLowered = false;
        setTimeout("restaurar()", 2000);
    }
    //restaurar();

</script>


<script>
    function callkeydownhandler(evnt) {
        var ev = (evnt) ? evnt : event;
        var code = (ev.which) ? ev.which : event.keyCode;



        // opener.document.formul.foco.value = 123; 
        // alert(opener.document.formul.foco.name);

        //alert("El cï¿½digo de la tecla pulsada es: " + code);

        if (code == 118)
            window.opener.parent.sig();
        if (code == 121)
            window.opener.parent.cal();
    }
    if (window.document.addEventListener) {
        window.document.addEventListener("keydown", callkeydownhandler, false);
    } else {
        window.document.attachEvent("onkeydown", callkeydownhandler);
    }
</script>



